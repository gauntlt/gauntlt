<html>
<head>
<title>Gruyere: Login</title>
[[include:base.css]][[/include:base.css]]
</head>

<body>
[[include:menubar.gtl]][[/include:menubar.gtl]]
<div>
<h2>Gruyere: Login</h2>
</div>

[[if:_message]]
<div class='message'>{{_message}}</div>
[[/if:_message]]

<div class='content'>
<form method='get' action='/{{_unique_id}}/login'>
<table><tr><td>
    User name:
  </td><td>
    <input type='text' name='uid'>
</td></tr><tr><td>
    Password:
  </td><td>
    <input type='password' name='pw'>
</td></tr><tr><td></td><td align='right'>
    <input type='submit' value='Login'>
</td></tr></table>
</form>
</div>
</body>

</html>
